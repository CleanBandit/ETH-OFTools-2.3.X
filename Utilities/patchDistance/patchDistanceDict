FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      patchDistanceDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// List of patch from which the yDist will be computed
sourcePatches
(
    ground
    SU_HonggCampus
    buildings
    south
);

// As the yDist field can be computed from various patches,
// you must specify a name extension. The field wil be named
// yDist_fieldNameExtension
fieldNameExtension    "ground";



